{
  "html_root_dir": "./data/html",
  "json_root_dir": "./data/json",
  "read_sequence": [
    "file"
  ],
  "write_content": true,
  "write_backup": false,
  "scrape_delay_ms": 1000,
  "parser_map": {
    "text/vnd.iba.cocktail-list+html": "gorgeous_stew.iba.parsers.IbaCocktailListParser",
    "text/vnd.iba.cocktail+html": "gorgeous_stew.iba.parsers.IbaCocktailParser"
  },
  "transformer_map": {
    "application/vnd.gorgeous-stew.iba-all-cocktails+json": "gorgeous_stew.iba.transformers.IbaCocktailListTransformer"
  }
}
